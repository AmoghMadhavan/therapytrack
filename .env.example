# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key
REACT_APP_SUPABASE_SERVICE_KEY=your-service-role-key

# OpenAI Configuration (Required for Premium AI features)
REACT_APP_OPENAI_API_KEY=your_openai_api_key

# Performance Configuration
REACT_APP_DISABLE_STRICT_MODE=false
REACT_APP_CACHE_TTL_MINUTES=15

# Security Configuration
REACT_APP_ENCRYPTION_SECRET=your_encryption_secret_key
REACT_APP_SESSION_TIMEOUT_MINUTES=30
REACT_APP_SESSION_WARNING_MINUTES=1

# Other environment variables
NODE_ENV=development 